# File: .env.example
# Version: v0.3.0
# --- Dev ports (ng serve + uvicorn --reload) ---
BACKEND_PORT=8000
FRONTEND_PORT_DEV=4200

# --- Prod port (Nginx) ---
NGINX_PORT=8080

# CORS origins (comma-separated or "*")
CORS_ORIGINS=*

# Database (SQLAlchemy URL). Default is a local SQLite file in the repo.
DB_URL=sqlite:///backend/app/data/cornstructor.db
